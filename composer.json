{
    "name": "dhii/iterator-base",
    "description": "Base concrete functionality for iterators",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.4 | ^7.0",
        "dhii/iterator-interface": "dev-release/0.1",
        "dhii/iterator-abstract": "dev-task/initial-classes",
        "dhii/data-key-value-aware-trait": "dev-task/initial-traits"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "^0.1",
        "codeclimate/php-test-reporter": "<=0.3.2"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\Iterator\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/iterator-interface"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/iterator-abstract"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/data-key-value-aware-trait"
        }
    ]
}
